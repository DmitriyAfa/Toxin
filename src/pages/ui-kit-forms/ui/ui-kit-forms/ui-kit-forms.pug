  link(rel='stylesheet' href=require('@/shared/ui/like/like.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/rating/rating.scss'))

block content
  +row({
    justify: "center",
    align: "center",
    gap: '20',
  })
    +rate-buttons()

block script
  script(src=require("@/app/app"))
  script(src=require("@/shared/ui/button/index.js"))
  script(src=require("@/shared/ui/like/index.js"))
  script(src=require("@/shared/ui/named-wrapper/index.js"))
  script(src=require("@/entities/dropdown/dropdown"))
  script(src=require("@/features/amenities-dropdown/amenities-dropdown"))
  script(src=require("@/features/guests-dropdown/guests-dropdown"))
  script(src=require("@/features/date-dropdown/date-dropdown"))