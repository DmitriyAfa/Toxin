block style
  link(rel='stylesheet' href=require('@/app/styles/index.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/flex/flex.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/form-wrapper/form-wrapper.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/input/input.scss'))
  link(rel='stylesheet' href=require('@/features/text-field/text-field.scss'))
  link(rel='stylesheet' href=require('@/entities/checkbox-list/checkbox-list.scss'))
  link(rel='stylesheet' href=require('@/features/date-dropdown/date-dropdown.scss'))
  link(rel='stylesheet' href=require('@/entities/dropdown/dropdown.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/button/button.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/bullet/bullet.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/toggle/toggle.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/radio/radio.scss'))
  link(rel='stylesheet' href=require('@/entities/bullet-list/bullet-list.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/named-wrapper/named-wrapper.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/text/text.scss'))
  link(rel='stylesheet' href=require('@/shared/ui/checkbox/checkbox.scss'))
