mixin header(props={})
  -
    const {
      navigationList=[
        {
          text: 'О нас',
          href: './error-page.html',
          active: true,
        },
        {
          text: 'Услуги',
          href: './error-page.html',
          active: false,
          links: [
            {
              text: 'Услуга 1',
              href: './error-page.html',
            },
            {
              text: 'Услуга 2',
              href: './error-page.html',
            },
            {
              text: 'Услуга 3',
              href: './error-page.html',
            },
          ],
        },
        {
          text: 'Вакансии',
          href: './error-page.html',
          active: false,
        },
        {
          text: 'Новости',
          href: './error-page.html',
          active: false,
        },
        {
          text: 'Соглашения',
          href: './error-page.html',
          active: false,
          links: [
            {
              text: 'Соглашение 1',
              href: './error-page.html',
            },
            {
              text: 'Соглашение 2',
              href: './error-page.html',
            },
            {
              text: 'Соглашение 3',
              href: './error-page.html',
            },
          ],
        },
      ],
      logged=false,
      userName='',
    }=props

  header.header
    .header__wrapper
      .header__body
        .header__logo
          img.logo(src=require('../../assets/icons/logo.svg'))
        .header__menu
          .header__navigation
            +navigation({
              navigationList: navigationList,
            })
          if logged
            p.header__logged #{userName}
          else
            .header__buttons
              +button-main-bordered({
                link:'login',
                text: 'войти',
              })
              +button-main({
                link:'registration',
                text: 'зарегистрироваться',
                size: 'biggest',
              })
